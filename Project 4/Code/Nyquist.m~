sys = tf([1.6],[1 2 1 0]);

nyquist(sys);

%% 

sys2 = tf([1.6], [1,1,0]);

nyquist(sys2)

%%

data = out.simout;

d = data.Data;
t = data.Time;

plot(t,d)

%%

y1 = -10:0.1:10;
y2 = -1.* y1;

y = [y1 y2];

%% 

k = 1.6;
M = 5;
alpha = 0.5;

syms A w

eq1 = w^2 - (2*k*M/(pi*A))*sqrt(1-alpha^2);
eq2 = w-w^3-4*M*alpha*k/(pi*A);
sol = solve(eq1,eq2);  

A   = double(sol.A);
w  = double(sol.w);


